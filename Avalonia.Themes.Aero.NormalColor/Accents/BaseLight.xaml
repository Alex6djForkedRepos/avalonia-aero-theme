<ResourceDictionary xmlns="https://github.com/avaloniaui"
       xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
       xmlns:sys="clr-namespace:System;assembly=netstandard">
  <SolidColorBrush x:Key="AeroForeground" Color="#FF000000"/>
  <SolidColorBrush x:Key="AeroDisabledForeground" Color="#FF7F7F7F"/>
  <SolidColorBrush x:Key="AeroWindowBackground" Color="#FFFCFCFC"/>



  <LinearGradientBrush x:Key="AeroToolTipBackground" StartPoint="0%,0%" EndPoint="0%,100%">
    <GradientStop Offset="0" Color="#FFFFFFFF"/>
    <GradientStop Offset="1" Color="#FFE4E5F0"/>
  </LinearGradientBrush>
  <SolidColorBrush x:Key="AeroToolTipBorderBrush" Color="#FF767676"/>



  <LinearGradientBrush x:Key="AeroButtonIdleBackground" StartPoint="0%,0%" EndPoint="0%,100%">
    <GradientStop Offset="0" Color="#FFF2F2F2"/>
    <GradientStop Offset="0.5" Color="#FFEBEBEB"/>
    <GradientStop Offset="0.5" Color="#FFDDDDDD"/>
    <GradientStop Offset="1" Color="#FFCFCFCF"/>
  </LinearGradientBrush>
  <SolidColorBrush x:Key="AeroButtonIdleOuterBorderBrush" Color="#FF707070"/>
  <LinearGradientBrush x:Key="AeroButtonIdleInnerBorderBrush" StartPoint="0%,0%" EndPoint="0%,100%">
    <GradientStop Offset="0" Color="#FFFCFCFC"/>
    <GradientStop Offset="1" Color="#FFF3F3F3"/>
  </LinearGradientBrush>


  <LinearGradientBrush x:Key="AeroButtonHoverBackground" StartPoint="0%,0%" EndPoint="0%,100%">
    <GradientStop Offset="0" Color="#FFEAF6FD"/>
    <GradientStop Offset="0.5" Color="#FFD9F0FC"/>
    <GradientStop Offset="0.5" Color="#FFBEE6FD"/>
    <GradientStop Offset="1" Color="#FFA7D9F5"/>
  </LinearGradientBrush>
  <SolidColorBrush x:Key="AeroButtonHoverOuterBorderBrush" Color="#FF3C7FB1"/>
  <LinearGradientBrush x:Key="AeroButtonHoverInnerBorderBrush" StartPoint="0%,0%" EndPoint="0%,100%">
    <GradientStop Offset="0" Color="#FFFAFDFE"/>
    <GradientStop Offset="1" Color="#FFE8F5FC"/>
  </LinearGradientBrush>

  <LinearGradientBrush x:Key="AeroButtonPressedBackground" StartPoint="0%,0%" EndPoint="0%,100%">
    <GradientStop Offset="0" Color="#FFE5F4FC"/>
    <GradientStop Offset="0.5" Color="#FFC4E5F6"/>
    <GradientStop Offset="0.5" Color="#FF98D1EF"/>
    <GradientStop Offset="1" Color="#FF68B3DB"/>
  </LinearGradientBrush>
  <SolidColorBrush x:Key="AeroButtonPressedOuterBorderBrush" Color="#FF2C628B"/>
  <LinearGradientBrush x:Key="AeroButtonPressedInnerBorderBrush" StartPoint="0%,0%" EndPoint="0%,100%">
    <GradientStop Offset="0" Color="#FF9EB0BA"/>
    <GradientStop Offset="0.5" Color="#FF9ABACB"/>
    <GradientStop Offset="0.5" Color="#FF78AAC5"/>
    <GradientStop Offset="1" Color="#FF68B3DB"/>
  </LinearGradientBrush>

  <SolidColorBrush x:Key="AeroButtonDisabledBackground" Color="#FFF4F4F4"/>
  <SolidColorBrush x:Key="AeroButtonDisabledOuterBorderBrush" Color="#FFADB2B5"/>
  <SolidColorBrush x:Key="AeroButtonDisabledInnerBorderBrush" Color="#FFFCFCFC"/>



  <SolidColorBrush x:Key="AeroCheckBoxIdleOuterBorderBrush" Color="#FF8E8F8F"/>
  <SolidColorBrush x:Key="AeroCheckBoxIdleMiddleBorderBrush" Color="#FFF4F4F4"/>
  <LinearGradientBrush x:Key="AeroCheckBoxIdleInnerBorderBrush" StartPoint="0%,0%" EndPoint="100%,100%">
    <GradientStop Offset="0" Color="#FFAEB3B9"/>
    <GradientStop Offset="1" Color="#FFE9E9EA"/>
  </LinearGradientBrush>
  <LinearGradientBrush x:Key="AeroCheckBoxIdleBackground" StartPoint="0%,0%" EndPoint="100%,100%">
    <GradientStop Offset="0" Color="#FFCBCFD5"/>
    <GradientStop Offset="1" Color="#FFF6F6F6"/>
  </LinearGradientBrush>
  <SolidColorBrush x:Key="AeroCheckBoxIdleCheckMarkBrush">#FF495F96</SolidColorBrush>

  <SolidColorBrush x:Key="AeroCheckBoxHoverOuterBorderBrush" Color="#FF5586A3"/>
  <SolidColorBrush x:Key="AeroCheckBoxHoverMiddleBorderBrush" Color="#FFDEF9FA"/>
  <LinearGradientBrush x:Key="AeroCheckBoxHoverInnerBorderBrush" StartPoint="0%,0%" EndPoint="100%,100%">
    <GradientStop Offset="0" Color="#FF79C6F9"/>
    <GradientStop Offset="1" Color="#FFCFECFD"/>
  </LinearGradientBrush>
  <LinearGradientBrush x:Key="AeroCheckBoxHoverBackground" StartPoint="0%,0%" EndPoint="100%,100%">
    <GradientStop Offset="0" Color="#FFB1DFFD"/>
    <GradientStop Offset="1" Color="#FFE7F7FE"/>
  </LinearGradientBrush>
  <SolidColorBrush x:Key="AeroCheckBoxHoverCheckMarkBrush">#FF042271</SolidColorBrush>

  <SolidColorBrush x:Key="AeroCheckBoxPressedOuterBorderBrush" Color="#FF2C628B"/>
  <SolidColorBrush x:Key="AeroCheckBoxPressedMiddleBorderBrush" Color="#FFC2E4F6"/>
  <LinearGradientBrush x:Key="AeroCheckBoxPressedInnerBorderBrush" StartPoint="0%,0%" EndPoint="100%,100%">
    <GradientStop Offset="0" Color="#FF5EB6F7"/>
    <GradientStop Offset="1" Color="#FFC1E6FC"/>
  </LinearGradientBrush>
  <LinearGradientBrush x:Key="AeroCheckBoxPressedBackground" StartPoint="0%,0%" EndPoint="100%,100%">
    <GradientStop Offset="0" Color="#FF9DD5FC"/>
    <GradientStop Offset="1" Color="#FFE0F4FE"/>
  </LinearGradientBrush>
  <SolidColorBrush x:Key="AeroCheckBoxPressedCheckMarkBrush">#FF3A5B98</SolidColorBrush>

  <SolidColorBrush x:Key="AeroCheckBoxDisabledBorderBrush" Color="#FFB1B1B1"/>
  <LinearGradientBrush x:Key="AeroCheckBoxDisabledBackground" StartPoint="0%,0%" EndPoint="100%,100%">
    <GradientStop Offset="0" Color="#FFF0F0F0"/>
    <GradientStop Offset="1" Color="#FFFBFBFB"/>
  </LinearGradientBrush>
  <SolidColorBrush x:Key="AeroCheckBoxDisabledCheckMarkBrush">#FF3A5B98</SolidColorBrush>


  <SolidColorBrush x:Key="AeroTabControlBackground" Color="#FFFFFFFF"/>
  <SolidColorBrush x:Key="AeroTabControlOuterBorderBrush" Color="#FFFFFFFF"/>
  <SolidColorBrush x:Key="AeroTabControlInnerBorderBrush" Color="#FF898C95"/>






  <sys:Double x:Key="AeroFontSizeSmall">10</sys:Double>
  <sys:Double x:Key="AeroFontSizeNormal">12</sys:Double>
  <sys:Double x:Key="AeroFontSizeLarge">16</sys:Double>

  <sys:Double x:Key="AeroScrollBarThickness">18</sys:Double>
  <sys:Double x:Key="AeroScrollBarThumbThickness">8</sys:Double>
</ResourceDictionary>